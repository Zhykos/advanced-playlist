{
  "name": "video-custom-feed",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./youtube-custom-feed/bin/www",
    "test": "jest --coverage"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^3.0.0",
    "lowdb": "1.0.0"
  },
  "devDependencies": {
    "debug": "~2.6.9",
    "nodemon": "^2.0.6",
    "jest": "^26.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "youtube-custom-feed/db.json"
    ],
    "delay": "2500"
  },
  "jest": {
    "testRegex": "./tests/.*.js$",
    "collectCoverageFrom": [
      "youtube-custom-feed/**/*.js",
      "!<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "./public/youtube-custom-feed/parameters.json": "<rootDir>/tests/tests-parameters.json"
    }
  }
}